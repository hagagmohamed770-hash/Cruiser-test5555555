{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    },
    "api/hello.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}